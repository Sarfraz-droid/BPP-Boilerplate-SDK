// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}


model User {
  id      String   @id @default(cuid())
  email   String   @unique
  name    String?
}

model Catalog {
  id      String   @id @default(cuid())  
  title   String   
  description String
  price   Int
  icon    String 
  category String
  quantity Int
}